<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>JavaShixunWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>FindAllStudent</servlet-name>
  	<servlet-class>com.situ.shixun.controller.FindAllStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>FindAllStudent</servlet-name>
  	<url-pattern>/findAllStudent</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>AddStudent</servlet-name>
  	<servlet-class>com.situ.shixun.controller.AddStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>AddStudent</servlet-name>
  	<url-pattern>/addStudent</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>DeleteStudent</servlet-name>
  	<servlet-class>com.situ.shixun.controller.DeleteStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>DeleteStudent</servlet-name>
  	<url-pattern>/deleteStudent</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
  	<servlet-name>ToUpdateStudent</servlet-name>
  	<servlet-class>com.situ.shixun.controller.ToUpdateStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ToUpdateStudent</servlet-name>
  	<url-pattern>/toUpdateStudent</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  	<servlet-name>UpdateStudent</servlet-name>
  	<servlet-class>com.situ.shixun.controller.UpdateStudentServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>UpdateStudent</servlet-name>
  	<url-pattern>/updateStudent</url-pattern>
  </servlet-mapping>
  
  
</web-app>